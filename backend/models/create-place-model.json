{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "city",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 3
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    },
    "city": {
      "type": "string",
      "minLength": 3
    },
    "country": {
      "type": "string",
      "minLength": 3
    }
  },
  "required": [
    "name",
    "latitude",
    "longitude",
    "city",
    "country"
  ],
  "additionalProperties": false
}
